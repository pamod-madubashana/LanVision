{
  "name": "netscope-network-scanner",
  "version": "1.0.0",
  "description": "Network Scanner Dashboard - Full-stack application with React frontend and Node.js backend",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev": "concurrently \"npm run dev --workspace=backend\" \"npm run dev --workspace=frontend\"",
    "dev:backend": "npm run dev --workspace=backend",
    "dev:frontend": "npm run dev --workspace=frontend",
    "build": "npm run build --workspace=backend && npm run build --workspace=frontend",
    "start": "npm start --workspace=backend",
    "test": "npm test --workspace=backend",
    "install:all": "npm install && npm install --workspace=backend && npm install --workspace=frontend"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "keywords": [
    "network-scanner",
    "nmap",
    "react",
    "nodejs",
    "typescript",
    "mongodb",
    "security",
    "dashboard"
  ],
  "author": "NetScope Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/LanVision.git"
  },
  "bugs": {
    "url": "https://github.com/your-username/LanVision/issues"
  },
  "homepage": "https://github.com/your-username/LanVision#readme"
}